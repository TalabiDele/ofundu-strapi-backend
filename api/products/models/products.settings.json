{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "via": "products",
      "collection": "categories"
    },
    "ranges": {
      "via": "products",
      "collection": "range"
    }
  }
}
